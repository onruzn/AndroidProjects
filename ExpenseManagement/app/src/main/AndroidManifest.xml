<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oyisoftware.onurr.expensetracking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.oyisoftware.onurr.expensetracking.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.oyisoftware.onurr.expensetracking.AddActivity" />
        <activity android:name="com.oyisoftware.onurr.expensetracking.HistoryActivity" />
        <activity android:name="com.oyisoftware.onurr.expensetracking.DetailActivity"></activity>
    </application>

</manifest>